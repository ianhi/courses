    "def phi(x,B):\n",
    "    return np.log((1+B* np.exp(-x))/(1-B* np.exp(-x)))\n",
    "x = np.linspace(0,10)\n",
    "plt.plot(x,phi(x,.5))\n",
    "plt.xlim([0,.3])"
   ]
